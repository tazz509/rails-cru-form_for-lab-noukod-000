<h1>Edit Song Form</h1>

<%= form_for @song do |f| %>
  <%= f.label :name %><br><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :artist_id %><br><br>
  <%= f.text_area :artist_id %><br><br>

  <%= f.label :genre_id %><br><br>
  <%= f.text_area :genre_id %><br><br>

  <%= f.submit %>
<% end %>
